include LICENSE
include README.md
include pyproject.toml
recursive-include jupyter-config *.json

include package.json
include install.json
include ts*.json
include yarn.lock

# Citations Style Language
graft csl-styles

graft jupyterlab_citation_manager/labextension
graft docs

# Javascript files
graft src
graft style
graft schema
graft tests
prune **/node_modules
prune lib
prune binder

# Patterns to exclude from any directory
global-exclude *~
global-exclude *.pyc
global-exclude *.pyo
global-exclude .git
global-exclude .ipynb_checkpoints
